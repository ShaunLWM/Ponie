{
	"plugins": [
		[
			"module-resolver",
			{
				"cwd": "babelrc",
				"alias": {
					"Constants": "./app/src/constants",
					"Components": "./app/src/components",
					"Core": "./app/src/core",
					"Pages": "./app/src/pages",
					"I18n": "./app/localization"
				}
			}
		],
		"@babel/plugin-transform-runtime"
	],
	"presets": ["@babel/preset-env", "@babel/preset-react"]
}
